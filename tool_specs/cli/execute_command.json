{
  "type": "function",
  "function": {
    "name": "execute_command",
    "description": "Execute a command line interface (CLI) command",
    "parameters": {
      "type": "object",
      "properties": {
        "command": {
          "type": "string",
          "description": "The CLI command to execute",
          "enum": [
            "ls",
            "pwd",
            "echo",
            "cat",
            "grep",
            "find",
            "curl",
            "wget",
            "git"
          ]
        },
        "args": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "Arguments for the CLI command"
        },
        "timeout": {
          "type": "integer",
          "description": "Timeout for the command in seconds"
        }
      },
      "required": [
        "command"
      ]
    }
  }
}